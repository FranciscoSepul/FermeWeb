@model FermeWeb1._0.Models.Cliente

@{
    ViewBag.Title = "Registrar";
}
<link href="~/css/Iframe.css" rel="stylesheet">
<link href="~/css/panelLo.css" rel="stylesheet">
<link href="~/css/panelAjustarResp.css" rel="stylesheet">
<iframe class="responsive-iframe" src="~/Header"></iframe>

<div class="container , center-block">
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <div class="panel panel-login">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            <li>@Html.ActionLink("LogIn", "LogIn", "Cliente")</li>
                        </div>
                        <div class="col-xs-6">
                            <li>@Html.ActionLink("Registrarme", "Registrar", "Cliente")</li>
                        </div>
                    </div>
                    <hr>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <form id="login-form" action="Registrar" method="post" role="form" style="display: block;">
                                <div class="form-group">
                                    <input type="text" name="run" id="run" tabindex="1" class="form-control" placeholder="Run" value="">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="dv" id="dv" tabindex="1" class="form-control" placeholder="Digito Verificador" value="">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="nombre" id="nombre" tabindex="1" class="form-control" placeholder="Nombre" value="">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="apellido" id="apellido" tabindex="1" class="form-control" placeholder="Apellido" value="">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="sexo" id="sexo" tabindex="1" class="form-control" placeholder="Sexo" value="">
                                </div>
                                <div class="form-group">
                                    <input type="email" name="correo" id="correo" tabindex="1" class="form-control" placeholder="Email" value="">
                                </div>
                                <div class="form-group">
                                    <input type="password" name="pass" id="pass" tabindex="2" class="form-control" placeholder="Contraseña">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="region" id="region" tabindex="2" class="form-control" placeholder="Region">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="comuna" id="comuna" tabindex="2" class="form-control" placeholder="Comuna">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="pasaje" id="pasaje" tabindex="2" class="form-control" placeholder="Pasaje">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="numero" id="numero" tabindex="2" class="form-control" placeholder="Numero Domicilio">
                                </div>
                                <div class="form-group">
                                    <input type="text" name="numeroDepto" id="numeroDepto" tabindex="2" class="form-control" placeholder="Numero Departamento">
                                </div>
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-sm-6 col-sm-offset-3">
                                            <input type="submit" name="register-submit" id="register-submit" tabindex="4" class="form-control btn btn-register" value="">
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
